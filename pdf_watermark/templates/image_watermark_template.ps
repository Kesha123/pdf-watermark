<<
   /EndPage
   {
     2 eq { pop false }
     {
         gsave
         translate
         /{{ font }} findfont 10 scalefont setfont
         newpath
         1 0 0 setrgbcolor 50 50 moveto 0 rotate
         ({{ text }}) false  charpath
         0 setlinewidth stroke
         grestore
         true
     } ifelse
   } bind
>>setpagedevice